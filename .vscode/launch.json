{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Launch",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/bin/Debug/net8.0/YourApp.dll",
        "args": [],
        "cwd": "${workspaceFolder}",
